#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f1f3b4dcb20 .scope module, "d_flip_flop_tb" "d_flip_flop_tb" 2 3;
 .timescale -9 -12;
v0x5f1f3b4f1a00_0 .var "clk", 0 0;
v0x5f1f3b4f1ac0_0 .var "d", 0 0;
v0x5f1f3b4f1b90_0 .net "q", 0 0, v0x5f1f3b4f17c0_0;  1 drivers
v0x5f1f3b4f1c90_0 .var "reset", 0 0;
S_0x5f1f3b4dccb0 .scope module, "dff" "d_flip_flop" 2 8, 3 1 0, S_0x5f1f3b4dcb20;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x5f1f3b4a3490_0 .net "clk", 0 0, v0x5f1f3b4f1a00_0;  1 drivers
v0x5f1f3b4f1700_0 .net "d", 0 0, v0x5f1f3b4f1ac0_0;  1 drivers
v0x5f1f3b4f17c0_0 .var "q", 0 0;
v0x5f1f3b4f1890_0 .net "reset", 0 0, v0x5f1f3b4f1c90_0;  1 drivers
E_0x5f1f3b4a3d90 .event posedge, v0x5f1f3b4f1890_0, v0x5f1f3b4a3490_0;
    .scope S_0x5f1f3b4dccb0;
T_0 ;
    %wait E_0x5f1f3b4a3d90;
    %load/vec4 v0x5f1f3b4f1890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5f1f3b4f17c0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5f1f3b4f1700_0;
    %assign/vec4 v0x5f1f3b4f17c0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5f1f3b4dcb20;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1f3b4f1a00_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5f1f3b4f1a00_0;
    %inv;
    %store/vec4 v0x5f1f3b4f1a00_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5f1f3b4dcb20;
T_2 ;
    %vpi_call 2 21 "$dumpfile", "d_flip_flop.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5f1f3b4dcb20 {0 0 0};
    %vpi_call 2 24 "$monitor", "Time=%t, clk=%b, reset=%b, d=%b, q=%b", $time, v0x5f1f3b4f1a00_0, v0x5f1f3b4f1c90_0, v0x5f1f3b4f1ac0_0, v0x5f1f3b4f1b90_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f1f3b4f1c90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1f3b4f1ac0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1f3b4f1c90_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f1f3b4f1ac0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1f3b4f1ac0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f1f3b4f1ac0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1f3b4f1ac0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f1f3b4f1c90_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1f3b4f1c90_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f1f3b4f1ac0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1f3b4f1ac0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "d_flip_flop_tb.v";
    "d_flip_flop.v";
